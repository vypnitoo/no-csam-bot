  "version": "1.0.0",
  "description": "Discord bot for preventing CSAM content sharing with progressive ban system",
  "main": "dist/bot/index.js",
  "scripts": {
    "dev": "tsx watch src/bot/index.ts",
    "build": "tsc",
    "start": "node dist/bot/index.js",
